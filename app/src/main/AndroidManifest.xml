<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dietcons">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Diet App"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DietPlan"></activity>

        <activity android:name=".NormalWeight"></activity>

        <activity android:name=".Obese"></activity>
        <activity android:name=".OverWeight"></activity>
        <activity android:name=".ViewItems"></activity>
        <activity android:name=".fooditemsDisplay"></activity>
    </application>

</manifest>